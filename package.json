{
  "name": "netflix-back",
  "version": "0.0.1",
  "main": "Index.ts",
  "type": "module",
  "scripts": {
    "tsc": "tsc --outDir build/",
    "nodemon": "dotenv -- nodemon build/Index.js",
    "dev": "concurrently --kill-others \"npm run tsc -- -w\" \"npm run nodemon\"",
    "migrator": "dotenv -- node build/Persistence/Migrator.js",
    "migrator:up": "dotenv -- node build/Persistence/Migrator.js up",
    "migrator:down": "dotenv -- node build/Persistence/Migrator.js down"
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "dotenv-cli": "^10.0.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.1.10",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "umzug": "^3.8.2"
  },
  "devDependencies": {
    "@sequelize/core": "^7.0.0-alpha.46",
    "@sequelize/postgres": "^7.0.0-alpha.46",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/sequelize": "^4.28.20",
    "concurrently": "^9.2.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
